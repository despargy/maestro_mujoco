        <framepos name="RL_Pos"   objtype="site" objname="RL_tip"  reftype="site" refname="imu"/>
        <framequat name="RL_Quat" objtype="site" objname="RL_tip"  reftype="site" refname="imu"/>

        <!-- Extra IMU sensor on foot tips -->
        <accelerometer name="FR_Acc" site="FR_tip" cutoff="10"/>
        <gyro name="FR_Gyro" site="FR_tip" />

        <accelerometer name="FL_Acc" site="FL_tip" cutoff="10"/>
        <gyro name="FL_Gyro" site="FL_tip"/>

        <accelerometer name="RR_Acc" site="RR_tip" cutoff="10"/>
        <gyro name="RR_Gyro" site="RR_tip"/>

        <accelerometer name="RL_Acc" site="RL_tip" cutoff="10"/>
        <gyro name="RL_Gyro" site="RL_tip"/>       


        <!-- Force sensors -->
        <force name="FR_foot_force" site="FR_tip"/> 
        <force name="FL_foot_force" site="FL_tip"/> 
        <force name="RR_foot_force" site="RR_tip"/> 
        <force name="RL_foot_force" site="RL_tip"/> 

        <!-- Velocity of the CoM -->
        <velocimeter name="Body_Vel" site="imu"/>

    </sensor>
    
    <contact>
        <pair  geom1="FR_tip" geom2="slippery_surface" condim="3" friction="0.8 0.02 0.01" /> 
        <pair  geom1="FL_tip" geom2="slippery_surface" condim="3" friction="0.8 0.02 0.01" />
        <pair  geom1="RR_tip" geom2="slippery_surface" condim="3" friction="0.8 0.02 0.01" />
        <pair  geom1="RL_tip" geom2="slippery_surface" condim="3" friction="0.8 0.02 0.01" />
    </contact> 


</mujoco>
    