        <pair  geom1="FR" geom2="slippery_surface" condim="3" friction="0.01 0.08 0.01" /> 
        <pair  geom1="FL" geom2="slippery_surface" condim="3" friction="0.01 0.08 0.01" />
        <pair  geom1="RR" geom2="slippery_surface" condim="3" friction="0.01 0.08 0.01" />
        <pair  geom1="RL" geom2="slippery_surface" condim="3" friction="0.01 0.08 0.01" />
